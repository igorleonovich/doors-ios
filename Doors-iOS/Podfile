# Uncomment the next line to define a global platform for your project
 platform :ios, '13.0'

target 'Doors-iOS' do
  # Comment the next line if you don't want to use dynamic frameworks
  inhibit_all_warnings!
  use_frameworks!

  pod 'MBProgressHUD'
  pod 'JWTDecode'
  pod 'R.swift'
  pod 'SnapKit'

  # Pods for Doors-iOS

  target 'Doors-iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Doors-iOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
